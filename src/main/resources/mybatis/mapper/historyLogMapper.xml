<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.colon.mattfolio.mapper.HistoryLogMapper">

    <insert id="insertErrorLog" parameterType="com.colon.mattfolio.model.HistoryLogRegisterRequestDto">
        INSERT INTO MF_COM_LOG_ERROR (
            REQUEST_INFO,
            CREATED_BY,
            CREATED_AT
        )
        VALUES (
            #{requestInfo},
            #{createdBy},
            #{createdAt}
        )
    </insert>

</mapper>